{
    "name": "drupal/apigee_m10n",
    "license": "GPL-2.0",
    "type": "drupal-module",
    "description": "Apigee Edge Monetization for Drupal",
    "require": {
        "php": ">=7.1",
        "drupal/apigee_edge": "^1.0-beta1",
        "commerceguys/intl": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "apigee/apigee-client-php": {
              "Fix developer ID issue when unsubscribing from a rate plan": "https://github.com/apigee/apigee-client-php/files/2627181/unsubscribe-rate-plan-developer-id.patch.txt"
            }
        }
    }
}
