{#
/**
 * @file
 *   GET /v1/organizations/{org}/developers/attributes
 * Response Code: 200
 *
#}

{
    "attribute": [
        {
            "name": "{{ attributes.billingTypeName|default('MINT_BILLING_TYPE') }}",
            "value": "{{ attributes.billingTypeValue|default('PREPAID') }}"
        },
        {
            "name": "{{ attributes.billingDeveloperName|default('MINT_DEVELOPER_ADDRESS') }}",
            "value": {
                "address1":"{{ attributes.address1|default('123 Main Street') }}",
                "address2":"{{ attributes.address2|default('') }}",
                "city":"{{ attributes.city|default('Mountain View') }}",
                "country":"{{ attributes.country|default('US') }}",
                "isPrimary":"{{ attributes.isPrimary|default('true') }}",
                "state":"{{ attributes.state|default('CA') }}",
                "zip":"{{ attributes.zip|default('94043') }}"
            }
        },
        {
            "name": "{{ attributes.billingLegalName|default('MINT_DEVELOPER_LEGAL_NAME') }}",
            "value": "{{ attributes.billingLegalValue|default('Joe Developer') }}"
        },
        {
            "name": "{{ attributes.billingDeveloperType|default('MINT_DEVELOPER_TYPE') }}",
            "value": "{{ attributes.billingDeveloperValue|default('UNTRUSTED') }}"
        }
    ]
}
