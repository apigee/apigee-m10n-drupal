{#
/**
 * @file
 *   GET /v1/mint/organizations/{org}/developers/{developer}/developer-balances
 *
 * Response Code: 200
 *
 * Variables:
 * - amount_aud: The AUD amount for the total balance.
 * - amount_usd: The USD amount for the total balance.
 * - developer: The developer user accound (used  by `transaction.json.twig`).
 */
#}
{
    "developerBalance" : [
        {
            "amount" : {{ amount_aud | default("16.0000") }},
            "chargePerUsage" : false,
            "id" : "f4a7b31b-90e3-4cfc-910c-517f36ea3477",
            "isRecurring" : false,
            "supportedCurrency" : {% include 'currency-aud.json.twig' %},
            "transaction": {% include 'transaction.json.twig' with { "currency": "AUD" } %},
            "usage" : 0
        },
        {
            "amount" : {{ amount_usd | default("16.0000") }},
            "chargePerUsage" : false,
            "id" : "f4a7b31b-90e3-4cfc-910c-517f36ea3477",
            "isRecurring" : false,
            "supportedCurrency" : {% include 'currency-usd.json.twig' with { "currency": "USD" } %},
            "transaction": {% include 'transaction.json.twig' %},
            "usage" : 0
        }
    ],
    "totalRecords" : 2
}
